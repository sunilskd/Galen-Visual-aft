@objects
    owners-graph-graph-control   xpath   //*[@id='content-view']/div[4]
    owners-graph-graph-control-graph-zoom    xpath   //*[@id='content-view']/div[4]/div[1]
    owners-graph-graph-control-percentage-filter xpath   //*[@id='content-view']/div[4]/div[2]
    owners-graph-graph-control-direct-filter xpath   //*[@id='content-view']/div[4]/div[3]
    owners-graph-graph-control-country-highlight xpath   //*[@id='content-view']/div[4]/div[4]
    owners-graph-graph-control-ubo-filter    xpath   //*[@id='content-view']/div[4]/div[5]



# Validating components in graph control are not over lapping
= Graph Control =
        owners-graph-graph-control-graph-zoom:
            above owners-graph-graph-control-percentage-filter 10px

        owners-graph-graph-control-graph-zoom:
            inside owners-graph-graph-control  10px top, 20px left, 32px right, 268 to 271px bottom

        owners-graph-graph-control-graph-zoom:
            above owners-graph-graph-control-percentage-filter 10px

        owners-graph-graph-control-percentage-filter:
            above owners-graph-graph-control-direct-filter 10px

        owners-graph-graph-control-direct-filter:
            above owners-graph-graph-control-country-highlight 10px

        owners-graph-graph-control-country-highlight:
            above owners-graph-graph-control-ubo-filter ~ 10px